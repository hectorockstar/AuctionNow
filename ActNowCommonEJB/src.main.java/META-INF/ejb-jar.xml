<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
	version="3.1">
	<display-name>ActNowCommonEJB</display-name>
	<ejb-client-jar>ActNowCommonClient.jar</ejb-client-jar>

	<enterprise-beans>
		<session>
			<ejb-name>CommonEjb</ejb-name>
			<business-local>com.auctionnow.ejb.ICommonEjbLocal</business-local>
			<business-remote>com.auctionnow.ejb.ICommonEjbRemote</business-remote>
			<ejb-class>com.auctionnow.ejb.CommonEjb</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>

</ejb-jar>